{
    "mcpServers": {
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ],
            "env": {
                "PLAYWRIGHT_CHROMIUM_EXECUTABLE_PATH": "/usr/bin/chromium-browser",
                "PLAYWRIGHT_BASE_URL": "http://localhost:80"
            }
        }
    }
}
